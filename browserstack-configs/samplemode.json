  "package": ["com.example.samplemode.uitests"],
  "coverage": true,
  "deviceLogs": true,
  "devices": ["Google Pixel 4-11.0"],
  "allowDeviceMockServer": "true",
  "project": "browserstacksample",
  "shards": {
    "numberOfShards": 2,
    "mapping": [
      {
